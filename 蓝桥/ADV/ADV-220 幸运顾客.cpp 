#include <iostream>
#include <queue>
using namespace std;
int main() {
    priority_queue<int, vector<int>, greater<int> > q;
    int n, t, cnt = 0, tt;
    cin >> n;
    while (n--) {
        cin >> t;
        if (t >= 0) {
            q.push(t);
        } else {
            vector<int> v(cnt);
            for (int i = 0; i < cnt; i++) {
                v[i] = q.top();
                q.pop();

            }
            cout << q.top() << endl;
            for (int i = 0; i < cnt; i++)
                q.push(v[i]);
            cnt++;
        }
    }
    return 0;
}