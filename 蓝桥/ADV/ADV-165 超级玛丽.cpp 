#include <iostream>
using namespace std;
int main() {
    int n, k, t, a[100] = {0}, f[30] = {0};
    int l = 1, r = 1, sum = 1;
    cin >> n >> k;
    a[n + 1] = -1, f[0] = 1, f[1] = 1;
    for (int i = 2; i <= 30; i++)
        f[i] = f[i - 1] + f[i - 2];
    for (int i = 1; i <= k; i++) {
        cin >> t;
        a[t] = -1;
    }
    for (int i = 1; i <= n + 1; i++) {
        if (a[i] == -1) {
            sum *= f[r - l];
            l = i + 1;
        } else {
            r = i;
        }
    }
    cout << sum;
    return 0;
}

