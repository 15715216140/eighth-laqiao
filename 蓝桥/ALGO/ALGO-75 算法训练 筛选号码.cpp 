#include <iostream>
#include <queue>
using namespace std;
int main() {
    deque<int> dq;
    int n, cnt = 1;
    cin >> n;
    for (int i = 1; i <= n; i++)
        dq.push_back(i);
    while (dq.size() > 1) {
        int t = dq.front();
        dq.pop_front();
        if (cnt == 1 || cnt % 3 != 0)
            dq.push_back(t);
        cnt++;
    }
    cout << dq.front();
    return 0;
}
