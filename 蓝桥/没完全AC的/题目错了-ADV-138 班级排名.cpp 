#include <algorithm>
#include <map>
#include <cmath>
#include <iostream>
#include <vector>

using namespace std;

int main() {
    int n, k;
    string s;
    cin >> n;
    for (int i = 0; i < n; i++)
        cin >> s;
    cin >> k;
    while (k--) {
        int t, tdada, cnt = 0;
        vector<int> v(n);
        for (int i = 0; i < n; i++) {
            cin >> t >> s;
            v[i] = t;
            if (s == "DaDa") tdada = t;
        }
        for (int i = 0; i < n; i++)
            if (v[i] > tdada) cnt++;
        cout << cnt + 1 << endl;
    }
    return 0;
}

