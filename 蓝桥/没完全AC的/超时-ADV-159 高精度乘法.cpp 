#include <iostream>
#include <algorithm>
#include <string>
#include <vector>
using namespace std;
string mul(string &s1, string &s2) {
    vector<int> v(s1.length() + s2.length(), 0);
    reverse(s1.begin(), s1.end());
    reverse(s2.begin(), s2.end());
    for (int i = 0; i < s1.length(); i++) {
        for (int j = 0; j < s2.length(); j++) {
            int t = (s1[i] - '0') * (s2[j] - '0') + v[i + j];//
            v[i + j] = t % 10;
            v[i + j + 1] += t / 10;
        }
    }
    int i;
    for (i = v.size() - 1; v[i] == 0; i--);
    string ans(i + 1, '0');
    for (int j = 0; j <= i; j++)
        ans[j] = v[i - j] + '0';
    if (ans == "") ans = "0";
    return ans;
}
int main() {
    string s1, s2;
    cin >> s1 >> s2;
    cout << mul(s1, s2);
    return 0;
}

